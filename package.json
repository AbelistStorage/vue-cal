{
  "name": "vue-cal",
  "version": "4.3.1",
  "description": "A Vue JS full calendar, no dependency, no BS. :metal:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "repository": "https://github.com/antoniandre/vue-cal",
  "homepage": "https://antoniandre.github.io/vue-cal",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antoniandre",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /vue-cal/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/vue-cal.css && rm ./dist/favicon.ico",
    "serve": "vite preview --root docs --base /vue-cal/",
    "lint": "vite lint"
  },
  "main": "dist/vue-cal.cjs.js",
  "module": "dist/vue-cal.es.js",
  "unpkg": "dist/vue-cal.es.js",
  "jsdelivr": "dist/vue-cal.es.js",
  "exports": {
    "./dist/vue-cal.css": "./dist/vue-cal.css",
    "./dist/vue-cal.cjs.js": "./dist/vue-cal.cjs.js",
    ".": {
      "import": "./dist/vue-cal.es.js",
      "require": "./dist/vue-cal.cjs.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@mdi/font": "^6.6.96",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "rollup": "^2.71.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.5.1",
    "simple-syntax-highlighter": "^2.2.0",
    "vite": "^2.9.5",
    "vite-plugin-pug": "^0.3.1",
    "vue-router": "^4.0.14",
    "wave-ui": "^2.40.2"
  },
  "peerDependencies": {
    "vue": "^2.6.14 || ^3.2.0"
  },
  "keywords": [
    "vuecal",
    "vue cal",
    "vue calendar",
    "full calendar",
    "calendar events",
    "vue",
    "vuejs",
    "vue 3",
    "vue 2",
    "ui"
  ]
}
